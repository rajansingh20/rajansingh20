//Pair cpp stl

#include<bits/stdc++.h>
using namespace std ;
int main(){
    pair<int,int> p ={1,2};
    cout<<p.first<<" "<<p.second<<endl;;
    pair<int,pair<int,int>> p1={1,{2,3}};
    cout<<p.first<<" "<<p1.second.first<<" "<<p1.second.second<<endl;;
    pair<pair<pair<int,int>,int>,pair<int,pair<pair<int,int>,int>>>p2={{{1,22},23},{24,{{21,34},4}}};
    cout<<p2.first.first.first<<endl;
    cout<<p2.second.second.second<<endl;;
    cout<<p2.second.second.first.second<<endl;;
    pair<int,int>arr[]={{1,2},{3,4},{2,5}};
    cout<<arr[1].second;
}
